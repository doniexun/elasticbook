<div class="pure-g">
  <div class="pure-u-7-8 center">
    <h1>Aliases</h1>
    <table class="pure-table pure-table-horizontal">
      <thead>
        <tr>
          <th>#</th>
          <th>IndexAlias</th>
        </tr>
      </thead>

      <tbody>
        {{range .results}}
        <tr>
          <td>{{.Index}}</td>
          <td>{{.Name}}</td>
        </tr>
        {{ end }}
      </tbody>
    </table>
  </div>
</div>
